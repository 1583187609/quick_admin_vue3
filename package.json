{
  "name": "Quick Admin Vue3",
  "type": "module",
  "author": "fanlichuan",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "contributes": [
    "you"
  ],
  "bugs": "https://gitee.com/fanlichuan/quick_admin_vue3/issues",
  "homepage": "https://gitee.com/fanlichuan/quick_admin_vue3/#/login",
  "description": "以快（计算机执行效率、开发效率）为核心的后台管理系统开发模板",
  "keywords": [
    "quick",
    "admin",
    "vue3",
    "ElementPlus",
    "TypeScript"
  ],
  "scripts": {
    "serve": "vite",
    "dev": "vite --mode dev",
    "test": "vite --mode test",
    "pre": "vite --mode pre",
    "prod": "vite --mode prod",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:test": "vue-tsc && vite build --mode test",
    "build:pre": "vue-tsc && vite build --mode pre",
    "build:prod": "vue-tsc && vite build --mode prod",
    "build:simple": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:fix": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:ls": "ls-lint",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,vue}\"",
    "prepare": "husky install",
    "api": "node ./scripts/api/index.js",
    "apisya": "node ./scripts/api/ya.js",
    "imgs": "node ./scripts/images.js",
    "doc": "node ./scripts/doc/index.js",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "dependencies": {
    "@mdit-vue/shared": "^2.1.3",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.3.2",
    "dayjs": "^1.11.7",
    "driver.js": "^1.3.1",
    "echarts": "^5.4.1",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.3.8",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.1",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.2",
    "vite-plugin-mock": "^2.9.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.2.4",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@ls-lint/ls-lint": "^2.2.3",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.58.0",
    "stylelint": "^16.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^4.9.3",
    "unplugin-generate-component-name": "^0.2.0",
    "vite": "^4.1.0",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vitepress": "^1.3.1",
    "vue-docgen-api": "^4.79.2",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass}": "stylelint --fix",
    "**/*": "prettier --write",
    "src/**": "ls-lint"
  }
}
